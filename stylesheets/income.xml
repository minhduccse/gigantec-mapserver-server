<?xml version="1.0" encoding="utf-8"?>

<!-- An ultra simple Mapnik stylesheet -->

<!DOCTYPE Map[]>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over" background-color="#00000000" buffer-size="256" maximum-extent="-20037508.34,-20037508.34,20037508.34,20037508.34">
    <Style name="50_percent_below">
        <Rule>
            <PolygonSymbolizer fill="#C8F08F" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>
    <Style name="50_percent">
        <Rule>
            <PolygonSymbolizer fill="#8CD211" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>
    <Style name="60_percent">
        <Rule>
            <PolygonSymbolizer fill="#5AA700" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>
    <Style name="70_percent">
        <Rule>
            <PolygonSymbolizer fill="#4C8400" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>
    <Style name="80_percent">
        <Rule>
            <PolygonSymbolizer fill="#144D14" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>
    <Style name="90_percent">
        <Rule>
            <PolygonSymbolizer fill="#0C2808" />
            <LineSymbolizer stroke="#999999" stroke-width="0" />
        </Rule>
    </Style>


    <Layer name="hcm_50_percent" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
        <StyleName>50_percent</StyleName>
        <Datasource>
            <Parameter name="type">shape</Parameter>
